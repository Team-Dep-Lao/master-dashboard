{
  "kind": "singleType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "default": "KDev Team"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "intro": {
      "type": "text"
    },
    "skill": {
      "displayName": "P Skill Section",
      "type": "component",
      "repeatable": true,
      "component": "common.p-skill-section"
    },
    "project": {
      "displayName": "P Project Section",
      "type": "component",
      "repeatable": true,
      "component": "common.p-project-section"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "contact": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
